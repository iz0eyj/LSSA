# Configurazione del server
PORT=3001
NODE_ENV=development

# Configurazione Garage-TraeUI API
GARAGE_API_URL=http://192.168.x.x:3903
GARAGE_ADMIN_TOKEN=- REPLACE -
GARAGE_TIMEOUT=30000

# Configurazione Garage S3 API
GARAGE_S3_ENDPOINT=http://192.168.x.x:3900
GARAGE_S3_ACCESS_KEY_ID=- REPLACE -
GARAGE_S3_SECRET_ACCESS_KEY=- REPLACE -
GARAGE_S3_REGION=garage

# CORS
CORS_ORIGIN=http://localhost:5173

# Rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info